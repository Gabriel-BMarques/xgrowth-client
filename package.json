{
  "name": "projecty-app-client",
  "version": "2.11.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod --build-optimizer=true --delete-output-path=true",
    "build-prod-patch": "npm run env -s && npm version patch && ng build --prod --build-optimizer=true --delete-output-path=true",
    "build-prod-minor": "npm run env -s && npm version minor && ng build --prod --build-optimizer=true --delete-output-path=true",
    "build-demo": "npm run env -s && ng build --configuration=demo --build-optimizer=true --delete-output-path=true",
    "build-demo-patch": "npm run env -s && npm version patch && ng build --configuration=demo --build-optimizer=true --delete-output-path=true",
    "build-demo-minor": "npm run env -s && npm version minor && ng build --configuration=demo --build-optimizer=true --delete-output-path=true",
    "deploy-prod": "npm run env -s && ng build --prod --build-optimizer=true --delete-output-path=true && cd www && az login --allow-no-subscriptions -u support@growinco.com -p 'iU87&sAJR&oo' && az webapp up --subscription Pay-As-You-Go --plan ASP-XGrowthResources-9f55 -n xgrowth-client-prod -l centralus --html && cd .. && rm -rf www",
    "deploy-demo": "npm run env -s && ng build --configuration=demo --build-optimizer=true --delete-output-path=true && cd www && az login --allow-no-subscriptions -u support@growinco.com -p 'iU87&sAJR&oo' && az webapp up --subscription Pay-As-You-Go --plan ASP-XGrowthResources-9f55 -n xgrowth-test -l centralus --html && cd .. && rm -rf www",
    "add_date": "npm run add_date_js && npm run add_date_css && npm run rm_bak_files",
    "add_date_js": "for i in www/*; do if [ -f $i ]; then LC_ALL=C sed -i.bak 's:js\":js?'$(date +%H%M%m%d%y)'\":g' $i; fi done",
    "rm_bak_files": "find www -name '*.bak' -exec rm -Rf {} \\;",
    "add_date_css": "sed -i.bak 's:css\":css?'$(date +%H%M%m%d%y)'\":g' www/index.html",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "start-mem": "npm run env -s && node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js",
    "serve-prod": "ng build --configuration=demo && npx http-server ./www -p 4200",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --configuration=demo --release --copy www",
    "cordova:build-demo": "ngx-scripts cordova build --device --debug --configuration=demo --copy www",
    "cordova:build-prod": "ngx-scripts cordova build --device --prod --release --copy www",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
